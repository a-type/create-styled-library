{
  "name": "styled-library",
  "version": "0.1.0",
  "description":
    "Tools for creating component libraries with React and styled-components",
  "main": "dist/index.js",
  "bin": {
    "styled-library": "bin/styled-library.js"
  },
  "engines": {
    "node": ">=6.0"
  },
  "scripts": {
    "test": "jest",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "build": "babel --copy-files --out-dir scripts scriptsSrc",
    "dev": "babel --watch --copy-files --out-dir scripts scriptsSrc"
  },
  "lint-staged": {
    "*{js,json,css}": [
      "prettier --write \"{scriptsSrc,util,__tests__}/**/*.js\"",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-type/styled-library.git"
  },
  "keywords": ["styled-components"],
  "author": "Grant Forrest <gaforres@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/a-type/styled-library/issues"
  },
  "homepage": "https://github.com/a-type/styled-library#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "prettier": "^1.7.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "change-case": "^3.0.1",
    "cross-spawn": "^5.1.0",
    "handlebars": "^4.0.10",
    "inquirer": "^3.2.3",
    "minimist": "^1.2.0",
    "plop": "^1.8.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-styleguidist": "^6.0.24",
    "webpack": "^3.6.0"
  }
}
